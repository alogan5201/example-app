  background-color: #555555;
}
.seo-navbar > .toggle > .icon:before,
.seo-navbar > .toggle > .icon:after {
  position: absolute;
  content: "";
}
.seo-navbar > .toggle > .icon:before {
  top: -8px;
}
.seo-navbar > .toggle > .icon:after {
  top: 8px;
}
.seo-navbar > .toggle.-active > .icon {
  background-color: transparent;
}
.seo-navbar > .toggle.-active > .icon:before {
  transform: translateY(8px) rotate(45deg);
}
.seo-navbar > .toggle.-active > .icon:after {
  transform: translateY(-8px) rotate(-45deg);
